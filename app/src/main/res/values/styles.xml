<resources>

<!-- Base application theme. -->
<style name="AppTheme" parent="Theme.AppCompat.Light">
    <!-- Customize your theme here. -->

    <item name="colorPrimary">@android:color/transparent</item>
    <item name="colorPrimaryDark">#D2691E</item>
    <item name="colorAccent">#D2691E</item>
    <!-- set Can use Activity transitions -->
    <item name="android:windowContentTransitions">true</item>
</style>
<style name="Translucent" parent="Theme.AppCompat.Light.NoActionBar">
    <item name="colorPrimaryDark">#D2691E</item>
    <item name="android:panelBackground">@android:color/transparent</item>
    <item name="android:windowBackground">@android:color/transparent</item>
    <item name="android:windowIsTranslucent">true</item>

</style>
    <style name="Transpa" parent="AppTheme">
        <item name="android:windowContentOverlay">@null</item>
        <item name="windowActionBarOverlay">true</item>
        <item name="colorPrimary">@android:color/transparent</item>
    </style>

    <style name="TextLabel" parent="Theme.AppCompat.Light">
    <!-- Label color in TRUE state and bar color FALSE and TRUE State -->
    <item name="colorControlActivated">#D2691E</item>
</style>
</resources>
